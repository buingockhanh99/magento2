<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\Delimiter" name="line1" template="Magento_Customer::account/navigation-delimiter.phtml">
                <arguments>
                    <argument name="sortOrder" xsi:type="number">25</argument>
                </arguments>
            </block>

            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="ac1">
                <arguments>
                    <argument name="path" xsi:type="string">customer_cs/account/</argument>
                    <argument name="label" xsi:type="string" translate="true">Account Dashboard</argument>
                    <argument name="sortOrder" xsi:type="number">20</argument>
                </arguments>
            </block>

        </referenceBlock>

    </body>
</page>



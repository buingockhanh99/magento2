<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="magenest_student_student" resource="default" engine="innodb" comment="magenest_student_student Table">
		<column xsi:type="smallint" name="student_id" padding="6" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="student_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		<column name="dob" nullable="true" xsi:type="date" comment="Date of Birth"/>
		<column name="class" nullable="true" xsi:type="text" comment="class"/>
		<column name="address" nullable="true" xsi:type="text" comment="address"/>
	</table>
</schema>
